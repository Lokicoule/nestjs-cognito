{
  "name": "@nestjs-cognito/core",
  "version": "0.1.0",
  "description": "Cognito Provider for NestJS",
  "author": "Loik Fekkai",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Lokicoule/nestjs-cognito-monorepo/core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lokicoule/nestjs-cognito-monorepo.git"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "test": "jest --config ./tests/jest-lib.json",
    "test:dev": "jest --config ./tests/jest-lib.json --watch",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --watch",
    "test:all": "npm run test && npm run test:e2e"
  },
  "bugs": {
    "url": "https://github.com/Lokicoule/nestjs-cognito-monorepo/issues"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.131.0",
    "@nestjs/common": "9.0.5",
    "@nestjs/testing": "9.0.5",
    "@types/jest": "^28.1.6"
  },
  "peerDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.131.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "./lib/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
