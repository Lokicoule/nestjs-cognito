{
  "name": "@nestjs-cognito/auth",
  "version": "0.1.1",
  "description": "Auth Module for NestJS and Cognito",
  "author": "Loik Fekkai",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Lokicoule/nestjs-cognito-monorepo/auth#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lokicoule/nestjs-cognito-monorepo.git"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "coverage": "jest --config ./tests/jest-lib.json --coverage && coveralls < coverage/lcov.info",
    "coverage:e2e": "jest --config ./tests/jest-e2e.json --coverage && coveralls < coverage/lcov.info",
    "coverage:all": "npm run coverage && npm run coverage:e2e",
    "test": "jest --config ./tests/jest-lib.json",
    "test:dev": "jest --config ./tests/jest-lib.json --watch",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --watch",
    "test:all": "npm run test && npm run test:e2e"
  },
  "bugs": {
    "url": "https://github.com/Lokicoule/nestjs-cognito-monorepo/issues"
  },
  "devDependencies": {
    "@nestjs-cognito/core": "^0.1.0",
    "@nestjs-cognito/testing": "^0.1.2",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/testing": "9.0.5"
  },
  "peerDependencies": {
    "@nestjs-cognito/core": "0.1.0",
    "@nestjs/jwt": "^9.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs-cognito/workspace": "file:../..",
    "cd": "^0.3.3"
  }
}
