  "bugs": {
    "url": "https://github.com/Lokicoule/nestjs-cognito/issues"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.744.0",
    "@nestjs-cognito/core": "workspace:*",
    "@nestjs/common": "11.0.8",
    "@nestjs/testing": "11.0.8"
  }
